{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "",
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./",
    "paths": {
      "@config/*": [
        "src/config/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@controlers/*": [
        "src/controlers/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
    },
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}